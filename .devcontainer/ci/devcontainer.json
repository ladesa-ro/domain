{
  "name": "@ladesa-ro/domain.devcontainer.ci",

  "build": {
    "dockerfile": "../../Containerfile",
    "target": "devcontainer",
    "context": "../../"
  },

  "shutdownAction": "stopContainer",

  "workspaceFolder": "/var/lib/ladesa-ro/ci/domain",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/var/lib/ladesa-ro/ci/domain,type=bind,consistency=cached"
}
